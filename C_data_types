#include <stdio.h>
#include <stdbool.h>
int main(){
	//this is  single line comment
	/*this 
	is 
	multi
	line
	comment*/
	int age=18;
	float gpa =9.3;
	double pi=3.1423456688790123456789;
	char grade='B';
	char name[ ]="TEJA VARDHAN";
	bool learning=true;
	printf("my age is %d\n",age);
	printf("my gpa is %.1f\n",gpa);
	printf(" pi is equal to %.15lf\n",pi);
	printf("my grade is %c\n",grade);
	printf("my name is %s\n",name);
	if(learning){
		printf("i am learning");}
	else{
	printf("i am not learning");}
	
	return 0;
}
	
	
